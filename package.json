{
  "name": "frontcore-web",
  "version": "0.0.1",
  "description": "A web based centralize tool to perform static code analysis for your projects",
  "author": "Ashwin Hegde <ashwin.hegde3@gmail.com>",
  "scripts": {
    "dist:clean": "rimraf dist",
    "coverage:clean": "rimrag coverage",
    "bundle:watch": "yarn run dist:clean && webpack --config \"./config/build/dev.webpack.config.js\" --progress -d --watch",
    "bundle:dev": "yarn run dist:clean && webpack --config \"./config/build/dev.webpack.config.js\" --progress -p -d",
		"build": "yarn run lint && yarn run dist:clean && yarn run coverage:clean && yarn run test:all && webpack --config \"./config/build/prod.webpack.config.js\" --progress -p",
    "test:all": "yarn run coverage:clean && jest && jest --coverage",
    "test:client": "jest",
    "test:coverage": "yarn run coverage:clean && jest --coverage",
    "lint": "eslint config src",
    "lint:fix": "eslint --fix config src"
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bootstrap": "^3.3.6",
    "brace": "^0.8.0",
    "classnames": "^2.2.3",
    "cross-env": "^3.1.4",
    "csslint": "^0.10.0",
    "es6-promise": "^3.2.1",
    "eslint": "^2.6.0",
    "events": "^1.1.0",
    "font-awesome": "^4.6.3",
    "fuse.js": "^2.5.0",
    "highcharts": "^4.2.5",
    "history": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdoc": "^3.4.3",
    "keymirror": "^0.1.1",
    "react": "^15.3.2",
    "react-ace": "^3.2.0",
    "react-bootstrap": "^0.29.5",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.5.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-tap-event-plugin": "^1.0.0",
    "react-treebeard": "^1.1.2",
    "react-widgets": "^3.3.1",
    "redux": "^3.4.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-thunk": "^2.1.0",
    "superagent": "^1.8.3",
    "to-string-loader": "^1.1.5",
    "underscore": "^1.8.3",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.4",
    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.24.0",
    "http-server": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "jest": "^12.1.1",
    "jest-cli": "^16.0.2",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.0.1",
    "react-test-renderer": "^15.3.2",
    "redux-devtools": "^3.2.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.3.1"
  },
  "jest": {
    "scriptPreprocessor": "./config/test/jest.preprocessor.config",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "collectCoverage": true,
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "https://github.com/Frontcore/frontcore-web",
  "bugs": {
    "url": "https://github.com/Frontcore/frontcore-web/issues"
  },
  "license": "MIT"
}
